@model WebAdmin.Models.CatTipoDevolucion

@{
    ViewData["Title"] = "Crear Género";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
desc
        <li class="breadcrumb-item">Información</li>
        <li class="breadcrumb-item">Catálogos</li>
        <li class="breadcrumb-item">Tipo Devolucion</li>
        <li class="breadcrumb-item active" aria-current="page">Control de Tipo Devolucion</li>
    </ol>
</nav>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TipoDevolucionDesc" class="control-label"></label>
                <input style="text-transform:uppercase" asp-for="TipoDevolucionDesc" class="form-control" />
                <span asp-validation-for="TipoDevolucionDesc" class="text-danger"></span>
            </div>
            <br>
            <div class="form-group">
                <input style="text-transform:uppercase" type="submit" value="Crear" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-secondary">Regresar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }

